<h1>Listing frogs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Quantity</th>
    <th>Price</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @frogs.each do |frog| %>
  <tr>
    <td><%= frog.name %></td>
    <td><%= frog.description %></td>
    <td><%= frog.quantity %></td>
    <td><%= frog.price %></td>
    <td><%= link_to 'Show', frog %></td>
    <td><%= link_to 'Edit', edit_frog_path(frog) %></td>
    <td><%= link_to 'Destroy', frog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Frog', new_frog_path %>
